// representation of binary tree

struct Node{
    int val;
    Node*left;
    Node*right;
    Node(int data){
      val=data;
      left=right=NULL;
    }
}

main(){
    struct Node *root=new Node(1);
    root->left=new Node(2);
    root->right=new Node(3);

}


// traversal techniques
   1) DFS (preorder , inorder , post order)

   2) BFS  (Level order traversal)